<p id="notice"><%= notice %></p>

<h1>Shops</h1>

<table>
  <thead>
    <tr>
      <th>Itemname</th>
      <th>Metakeywords</th>
      <th>Metadescription</th>
      <th>Metaurl</th>
      <th>Metaimage</th>
      <th>Content</th>
      <th>Price</th>
      <th>Available</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shops.each do |shop| %>
      <tr>
        <td><%= shop.itemname %></td>
        <td><%= shop.metakeywords %></td>
        <td><%= shop.metadescription %></td>
        <td><%= shop.metaurl %></td>
        <td><%= shop.metaimage %></td>
        <td><%= shop.content %></td>
        <td><%= shop.price %></td>
        <td><%= shop.available %></td>
        <td><%= link_to 'Show', shop %></td>
        <td><%= link_to 'Edit', edit_shop_path(shop) %></td>
        <td><%= link_to 'Destroy', shop, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shop', new_shop_path %>
