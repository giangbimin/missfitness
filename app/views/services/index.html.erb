<p id="notice"><%= notice %></p>

<h1>Services</h1>

<table>
  <thead>
    <tr>
      <th>Survicename</th>
      <th>Metakeywords</th>
      <th>Metadescription</th>
      <th>Metaurl</th>
      <th>Metaimage</th>
      <th>Content</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @services.each do |service| %>
      <tr>
        <td><%= service.survicename %></td>
        <td><%= service.metakeywords %></td>
        <td><%= service.metadescription %></td>
        <td><%= service.metaurl %></td>
        <td><%= service.metaimage %></td>
        <td><%= service.content %></td>
        <td><%= service.active %></td>
        <td><%= link_to 'Show', service %></td>
        <td><%= link_to 'Edit', edit_service_path(service) %></td>
        <td><%= link_to 'Destroy', service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service', new_service_path %>
